@model List<school.Models.StudentModel>

<a asp-action="Create" class="btn btn-success">Create</a>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.First().Id)</th>
        <th>@Html.DisplayNameFor(model => model.First().Name)</th>
        <th>@Html.DisplayNameFor(model => model.First().Age)</th>
        <th>@Html.DisplayNameFor(model => model.First().CourseName)</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>@item.CourseName</td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id" class="btn btn-primary">Update</a>
                <a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>
            </td>
        </tr>
    }
</table>
